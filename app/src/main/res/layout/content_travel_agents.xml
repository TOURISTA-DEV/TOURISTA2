<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="60dp"
    tools:context="com.example.rehan.tourista.Activities.TravelAgentsActivity"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">

    <!-- TODO: Update blank fragment layout -->

    <EditText
        android:layout_marginTop="5dp"
        android:hint="Search"
        android:gravity="center"
        android:textColorHint="#8e7878"
        android:textSize="15dp"
        android:id="@+id/e1"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:inputType="textImeMultiLine" />

    <android.support.v7.widget.CardView
        android:id="@+id/card"
        android:layout_height="180dp"
        android:layout_width="match_parent"
        card_view:cardCornerRadius="5dp"
        card_view:cardBackgroundColor="@color/colorPrimary"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="6dp"
        android:layout_below="@+id/e1"
        android:layout_alignStart="@+id/card1">


        <!--<ImageView-->
        <!--android:layout_width="400dp"-->
        <!--android:layout_height="150dp"-->
        <!--android:src="@drawable/aa"/>-->

        <TextView
            android:id="@+id/t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:text="Start:  Lahore"
            android:textColor="#fdfbfb"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/t2"
            android:text="Agency:  Tourista Tours"
            android:textColor="#fdfbfb"
            android:textSize="20dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="70dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <TextView
            android:id="@+id/t3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="40dp"
            android:text="Duration:  1/12/2018 -- 10/12/2018"
            android:textColor="#fdfbfb"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/t5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="200dp"
            android:text="Dest:  Naraan"
            android:textColor="#fdfbfb"
            android:textSize="20dp"
            />

        <Button
            android:id="@+id/details_btn_travel_agent_fragment_xml"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="120dp"
            android:background="@drawable/custom_background_white"
            android:text="Details"
            android:textColor="@color/colorPrimary"

            />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card1"
        android:layout_height="180dp"
        android:layout_width="match_parent"
        card_view:cardCornerRadius="5dp"
        card_view:cardBackgroundColor="@color/colorPrimary"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/card"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp">


        <!--<ImageView-->
        <!--android:layout_width="400dp"-->
        <!--android:layout_height="150dp"-->
        <!--android:src="@drawable/feedback"/>-->

        <TextView
            android:id="@+id/t10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:text="Start:  Multan"
            android:textColor="#fdfbfb"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/t9"
            android:text="Agency:  Drt Tours and Travel"
            android:textColor="#fdfbfb"
            android:textSize="20dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="70dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <TextView
            android:id="@+id/t8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="40dp"
            android:text="Duration:  1/12/2018 -- 10/12/2018"
            android:textColor="#fdfbfb"
            android:textSize="20dp" />

        <Button
            android:id="@+id/button"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="120dp"
            android:background="@drawable/custom_background_white"
            android:text="Details"

            android:textColor="@color/colorPrimary" />

        <TextView
            android:id="@+id/t7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            android:text="Dest:  Muree"
            android:textColor="#fdfbfb"
            android:textSize="20dp" />

    </android.support.v7.widget.CardView>


</RelativeLayout>
